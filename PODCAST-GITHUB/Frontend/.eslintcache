           exact\n          />\n        </div>\n\n        <BottomNav />\n\n        <ProtectedRoute\n          path=\"/admin/contents\"\n          isAdmin={true}\n          component={ContentsList}\n          exact\n        />\n        <ProtectedRoute\n          path=\"/admin/content\"\n          isAdmin={true}\n          component={NewContent}\n          exact\n        />\n        <ProtectedRoute\n          path=\"/admin/content/:id\"\n          isAdmin={true}\n          component={UpdateContent}\n          exact\n        />\n\n        <ProtectedRoute\n          path=\"/admin/users\"\n          isAdmin={true}\n          component={UsersList}\n          exact\n        />\n\n        <ProtectedRoute\n          path=\"/admin/user/:id\"\n          isAdmin={true}\n          component={UpdateUser}\n          exact\n        />\n\n        {/* quybuiquang */}\n        {/* 09/042025 */}\n        <Route\n          path=\"/admin/manage-booking-content\"\n          component={ManageBookingContent}\n        />\n        <Route path=\"/admin/manage-bookings\" component={ManageBooking} />\n\n        <div style={{ height: \"85px\" }}></div>\n\n        {/* {!loading && (!isAuthenticated || user.typeRole !== 'admin') && (\n          <Footer />\n        )} */}\n\n        {/*<Footer />*/}\n      </div>\n      <ToastContainer\n        position=\"top-right\"\n        autoClose={1000}\n        hideProgressBar={false}\n        newestOnTop={false}\n        closeOnClick={false}\n        rtl={false}\n        pauseOnFocusLoss\n        draggable={false}\n        pauseOnHover={false}\n        theme=\"light\"\n        transition={Slide}\n      />\n    </Router>\n  );\n};\n\nexport default App;\n","/home/quy/dev/nodeJS/podcastTheO/PODCAST-GITHUB/Frontend/src/axios.js",[],"/home/quy/dev/nodeJS/podcastTheO/PODCAST-GITHUB/Frontend/src/reducers/contentReducers.js",[],"/home/quy/dev/nodeJS/podcastTheO/PODCAST-GITHUB/Frontend/src/components/Home.js",[],"/home/quy/dev/nodeJS/podcastTheO/PODCAST-GITHUB/Front